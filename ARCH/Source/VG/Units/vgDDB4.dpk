package vgDDB4;

{$R *.RES}
{$ALIGN ON}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $00400000}
{$DESCRIPTION 'VG Library 4.0 DB Components'}
{$DESIGNONLY}
{$IMPLICITBUILD OFF}

requires
  dcldb40,
  vgDVCL4;

contains
  vgDBRg in 'vgDBRg.pas',
  vgDBUtl in 'vgDBUtl.pas',
  vgBDEUtl in 'vgBDEUtl.pas',
  vgDBCtrl in 'vgDBCtrl.pas',
  vgDBTree in 'vgDBTree.pas',
  vgBDECon in 'vgBDECon.pas',
  vgConvEd in 'vgConvEd.pas',
  vgDBConv in 'vgDBConv.pas',
  vgDBRes in 'vgDBRes.pas',
  ConvAdd in 'ConvAdd.pas' {ConvertItemsAddForm},
  vgDBPrms in 'vgDBPrms.pas',
  vgDB in 'vgDB.pas',
  vgBDE in 'vgBDE.pas',
  ExplrDB in 'ExplrDB.pas',
  ExplrBDE in 'ExplrBDE.pas';

end.
